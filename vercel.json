{
  "headers": [
    {
      "source": "/sw-(.*)\\.js",
      "headers": [
        { "key": "Cache-Control", "value": "no-store" }
      ]
    },
    {
      "source": "/data/(.*)\\.json",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache" }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
